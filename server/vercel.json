{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "dist/**"
                ],
                "maxDuration": 60
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.js",
            "methods": ["GET", "POST", "OPTIONS"]
        }
    ],
    "functions": {
        "server.js": {
            "maxDuration": 60,
            "memory": 1024
        }
    }
}